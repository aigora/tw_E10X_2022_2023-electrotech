                        k++;
                    }
                }
                    int i, N, u;
                    u=0;
                    N=5;
                    float minimo;
                    minimo = Energias[N].datos[12];

                    //PARA LOS DOS AÑOS
                    for (i=0;i<24;i++)
                    {
                        if (Energias[N].datos[i]<minimo)
                        {
                            minimo=Energias[N].datos[i];
