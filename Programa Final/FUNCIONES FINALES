        if(pf == NULL)
            {
                printf("Error al encontrar el archivo");
            }
        else
            {
                for(i=0; i<4; i++)
                  {
                    fscanf(pf, "%c", &aux1);
                    while(aux1 != '\n')
                        {
                            fscanf(pf, "%c", &aux1);
                        }
                  }

                //AQUI TENGO QUE LEER LAS FECHAS
                char coma1, barra;//auxiliares para almacenar lo que no me interesa

                fecha vfecha[24];

                char aux4[100];

                fscanf(pf, "%[^,]", &aux4);//para que lea la palabra antes hasta la coma

